[{"id": 82, "type": "t", "title": "Natural & Broad Loom", "description": "", "image": "default-rug.png", "parent_group": ""}, {"id": 81, "type": "t", "title": "Traditional", "description": "", "image": "rug-groups/s480386452440936931_p22_i1_w640.jpeg", "parent_group": ""}, {"id": 80, "type": "t", "title": "Ethnic", "description": "", "image": "rug-groups/s480386452440936931_p61_i1_w640_1.jpeg", "parent_group": ""}, {"id": 79, "type": "t", "title": "Decorative", "description": "", "image": "rug-groups/s480386452440936931_p3_i8_w600.png", "parent_group": ""}, {"id": 78, "type": "t", "title": "Sustainable", "description": "", "image": "rug-groups/s480386452440936931_p1064_i1_w640.jpeg", "parent_group": ""}, {"id": 77, "type": "t", "title": "Furniture / Accessories", "description": "", "image": "rug-groups/s480386452440936931_p350_i1_w640.jpeg", "parent_group": ""}, {"id": 76, "type": "t", "title": "Gallery Size", "description": "", "image": "rug-groups/s480386452440936931_p671_i1_w576.jpeg", "parent_group": ""}, {"id": 75, "type": "t", "title": "Runners", "description": "", "image": "rug-groups/s480386452440936931_p120_i1_w640.jpeg", "parent_group": ""}, {"id": 74, "type": "a", "title": "Custom Order", "description": "", "image": "rug-groups/img-4540_orig.jpg", "parent_group": 64}, {"id": 73, "type": "a", "title": "Swedish / Scandinavian Inspired", "description": "", "image": "rug-groups/s480386452440936931_p408_i2_w640.jpeg", "parent_group": 64}, {"id": 72, "type": "a", "title": "Afghan", "description": "", "image": "rug-groups/s480386452440936931_p1137_i1_w640.jpeg", "parent_group": 71}, {"id": 71, "type": "a", "title": "Modern Flatweave", "description": "", "image": "rug-groups/s480386452440936931_p17_i1_w640.jpeg", "parent_group": 64}, {"id": 70, "type": "a", "title": "Modern", "description": "", "image": "rug-groups/s480386452440936931_p1069_i1_w640.jpeg", "parent_group": 64}, {"id": 69, "type": "a", "title": "Antique Inspired / Recreation", "description": "", "image": "rug-groups/s480386452440936931_p771_i1_w568.jpeg", "parent_group": 64}, {"id": 68, "type": "a", "title": "African Tuareg Mat", "description": "", "image": "rug-groups/s480386452440936931_c131_i2_w500.jpeg", "parent_group": 65}, {"id": 67, "type": "a", "title": "Vintage Moroccan", "description": "", "image": "rug-groups/s480386452440936931_p696_i1_w576.jpeg", "parent_group": 65}, {"id": 66, "type": "a", "title": "High Atlas Collection", "description": "", "image": "rug-groups/s480386452440936931_p892_i1_w640.jpeg", "parent_group": 65}, {"id": 65, "type": "a", "title": "Moroccan", "description": "Moroccan rugs, crafted by women in the interior plains and mountains on fixed-heddle looms can vary greatly depending on the tribes that weave them. Nonetheless, they all use severely geometric Moroccan decoration, sometimes in muted tones, sometimes almost monochrome, and sometimes richly colorful tones and asymmetrical compositions without borders. The raw material of the Moroccan rugs is black or white sheep's wool, used as is or dyed with plants or minerals found in the areas where the carpets are woven. in the upper regions, others are often used while in the plains of Marrakesh, madder provides brilliant reds. Moroccan rugs provide designers with timeless, unique, and functional works of art for the modern-day home. Each rug is a primitive abstraction that is completely original to the weaver of the rug. Recognizing the beauty of these rugs in the modern environment were notable designers and architects as Charles and Ray Eames, Le Corbusier, and Frank Lloyd Wright.", "image": "rug-groups/s480386452440936931_p882_i1_w640.jpeg", "parent_group": 64}, {"id": 64, "type": "a", "title": "Contemporary", "description": "Our collection of modern,  transitional, and contemporary rugs and carpets are handwoven in  Afghanistan, Egypt, India, Pakistan, Turkey, and Nepal. We source the finest wools and silks available, infusing traditional weaving techniques with modern design aesthetics. Complete customization is available for a specific design, color, and size requirements.", "image": "rug-groups/s480386452440936931_p175_i1_w640.jpeg", "parent_group": ""}, {"id": 63, "type": "a", "title": "Other", "description": "", "image": "rug-groups/s480386452440936931_p61_i1_w640.jpeg", "parent_group": 53}, {"id": 62, "type": "a", "title": "Tulu", "description": "", "image": "rug-groups/s480386452440936931_p79_i1_w640.jpeg", "parent_group": 53}, {"id": 61, "type": "a", "title": "Scandinavian", "description": "", "image": "rug-groups/s480386452440936931_p191_i1_w640_1.jpeg", "parent_group": 56}, {"id": 60, "type": "a", "title": "Swedish", "description": "", "image": "rug-groups/s480386452440936931_p401_i2_w600_1.jpeg", "parent_group": 56}, {"id": 59, "type": "a", "title": "Judith Johansson", "description": "", "image": "default-rug.png", "parent_group": 56}, {"id": 58, "type": "a", "title": "Marta Mass Fjetterstrom", "description": "", "image": "default-rug.png", "parent_group": 56}, {"id": 57, "type": "a", "title": "Ingegerd Silow", "description": "", "image": "default-rug.png", "parent_group": 56}, {"id": 56, "type": "a", "title": "Swedish / Scandinavian", "description": "Much like the dutch renaissance painters of the sixteenth century took inspiration from their Italian predecessors, twentieth-century Swedish rug designers took inspiration from imported textiles and incorporated them into their own artistic language. simple geometric patterns and vignettes from everyday life are woven through this form of folk art, and what began as a long pile hand-knotted coverlet, transformed over time into flatweave designs. Purely ornamental in nature; the superior craftsmanship, simplicity, and purity of design give these floor pieces, dating from the 1920s to 1950s, a timeless relevance, and contemporary desirability.", "image": "rug-groups/s480386452440936931_p33_i1_w640.jpeg", "parent_group": 53}, {"id": 55, "type": "a", "title": "Over-Dyed Distressed", "description": "", "image": "rug-groups/s480386452440936931_p41_i2_w640.jpeg", "parent_group": 53}, {"id": 54, "type": "a", "title": "Kilim / Flat Weave", "description": "Kilims are a pileless floor covering handwoven by tapestry techniques in Anatolia, the Balkans, and parts of Iran. in the rest of Iran, the Caucasus, and Turkistan, the name for similar pieces is pala. In most kilims, a slit occurs wherever two colors meet along a vertical line in the pattern, but in a few Karabagh or south caucasian pieces, interlocking methods are employed in order to minimize these slits. The Turks have produced the largest kilims, usually in two narrow pieces joined, as well as small ones and a multitude of prayer kilims. as a prayer rug, which is carried about with the worshiper, the light and extremely flexible kilim offers obvious advantages. In turkish kilims, cotton is often used for the white areas, and small details may be brocaded. The kilims of the southern Balkans began as close copies of Anatolian types but have gradually developed into individual styles, such as the black, red, and white kilims of the pilot. in Romania, also, there are varied local fashions, progressively less oriental in color and pattern as the distance from turkey increases. The name kilim is also given to a variety of brocaded, embroidered, warp-faced, and other flat-woven rugs and bags.", "image": "rug-groups/s480386452440936931_p59_i1_w640.jpeg", "parent_group": 53}, {"id": 53, "type": "a", "title": "Vintage", "description": "Our collection of vintage rugs, ranging from Moroccan Beni our rains to Turkish kilims, are true reflections of the times in which they were created. stylistically, one can see the influence of the modernist movement. Both abstract and minimalistic, each piece is an expression of experimentation and innovation, celebrating individualism and creativity.", "image": "rug-groups/s480386452440936931_p20_i1_w640.jpeg", "parent_group": ""}, {"id": 52, "type": "a", "title": "Oushak", "description": "", "image": "rug-groups/s480386452440936931_p133_i2_w640.jpeg", "parent_group": 46}, {"id": 51, "type": "a", "title": "Kilim", "description": "", "image": "rug-groups/s480386452440936931_p270_i1_w640.jpeg", "parent_group": 46}, {"id": 50, "type": "a", "title": "Kaysari", "description": "", "image": "rug-groups/s480386452440936931_p226_i1_w640.jpeg", "parent_group": 46}, {"id": 49, "type": "a", "title": "Hereke", "description": "", "image": "default-rug.png", "parent_group": 46}, {"id": 48, "type": "a", "title": "Sivas", "description": "", "image": "rug-groups/s480386452440936931_p261_i2_w640.jpeg", "parent_group": 46}, {"id": 47, "type": "a", "title": "Ghiordes", "description": "", "image": "rug-groups/s480386452440936931_p1264_i3_w640.jpeg", "parent_group": 46}, {"id": 46, "type": "a", "title": "Other", "description": "", "image": "rug-groups/s480386452440936931_p91_i1_w640.jpeg", "parent_group": 1}, {"id": 45, "type": "a", "title": "Chinese", "description": "", "image": "rug-groups/s480386452440936931_p245_i1_w640.jpeg", "parent_group": 1}, {"id": 44, "type": "a", "title": "Italian", "description": "", "image": "default-rug.png", "parent_group": 37}, {"id": 43, "type": "a", "title": "Bessarabian / Ukrainian", "description": "", "image": "default-rug.png", "parent_group": 37}, {"id": 42, "type": "a", "title": "Donegal / Arts and Crafts", "description": "", "image": "default-rug.png", "parent_group": 37}, {"id": 41, "type": "a", "title": "Spanish", "description": "", "image": "rug-groups/s480386452440936931_p241_i1_w640.jpeg", "parent_group": 37}, {"id": 40, "type": "a", "title": "French Savonnerie", "description": "", "image": "rug-groups/s480386452440936931_p1001_i1_w640.jpeg", "parent_group": 37}, {"id": 39, "type": "a", "title": "French Deco / Aubusson", "description": "", "image": "rug-groups/s480386452440936931_p206_i1_w640.jpeg", "parent_group": 37}, {"id": 38, "type": "a", "title": "Tapestry", "description": "", "image": "rug-groups/s4